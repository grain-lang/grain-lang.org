<% var githubUrl = 'https://github.com/grain-lang/grain-lang.github.io/blob/docs-main/' + config.project + '_source/' + page.path.replace(/\.html$/, '.md'); %>
<% var next = next(site, page); %>
<% var prev = prev(site, page); %>
<section id="hero">
  <nav id="nav">
    <div class="brand-wrapper">
      <button id="sidebar-toggle">
        <svg class="nav-menu-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 559 372"><defs><style>.a{fill:#fff;}</style></defs><title>menu</title><rect class="a" width="559" height="49" rx="24.5" ry="24.5"/><rect class="a" x="205" y="323" width="354" height="49" rx="24.5" ry="24.5"/><rect class="a" x="97" y="156" width="462" height="49" rx="24.5" ry="24.5"/></svg>
      </button>
      <a href="/"><img class="nav-logo" src="https://grain-lang.org/images/grain_logo_white.svg"></a>
    </div>
    <div class="nav-links">
      <a href="/docs" class="nav-link heading">Guide</a>
      <a href="/docs/intro" class="nav-link heading">Documentation</a>
      <a href="https://github.com/grain-lang/grain/graphs/contributors" class="nav-link heading">Community</a>
    </div>
    <div class="nav-social-links">
      <a href="https://github.com/grain-lang/grain" class="nav-link"><i class="fab fa-github nav-social-link"></i></a>
      <a href="https://twitter.com/grain_lang" class="nav-link"><i class="fab fa-twitter nav-social-link"></i></a>
    </div>
  </nav>
</section>
<div class="page-wrapper">
  <div id="all-content-wrapper">
    <div id="sidebar-mobile">
      <%- partial('_partial/sidebar') %>
    </div>
    <%- partial('_partial/sidebar') %>
    <div class="page-content-wrapper">
      <div class="page-content">
        <h1 class="page-title"><%- page.title %></h1>
        <a target="_blank" href="<%- githubUrl %>" class="github-link button button-secondary">
          <svg class="github-link-svg" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 499.36" focusable="false"><title>GitHub</title><path d="M256 0C114.64 0 0 114.61 0 256c0 113.09 73.34 209 175.08 242.9 12.8 2.35 17.47-5.56 17.47-12.34 0-6.08-.22-22.18-.35-43.54-71.2 15.49-86.2-34.34-86.2-34.34-11.64-29.57-28.42-37.45-28.42-37.45-23.27-15.84 1.73-15.55 1.73-15.55 25.69 1.81 39.21 26.38 39.21 26.38 22.84 39.12 59.92 27.82 74.5 21.27 2.33-16.54 8.94-27.82 16.25-34.22-56.84-6.43-116.6-28.43-116.6-126.49 0-27.95 10-50.8 26.35-68.69-2.63-6.48-11.42-32.5 2.51-67.75 0 0 21.49-6.88 70.4 26.24a242.65 242.65 0 0 1 128.18 0c48.87-33.13 70.33-26.24 70.33-26.24 14 35.25 5.18 61.27 2.55 67.75 16.41 17.9 26.31 40.75 26.31 68.69 0 98.35-59.85 120-116.88 126.32 9.19 7.9 17.38 23.53 17.38 47.41 0 34.22-.31 61.83-.31 70.23 0 6.85 4.61 14.81 17.6 12.31C438.72 464.97 512 369.08 512 256.02 512 114.62 397.37 0 256 0z" fill="currentColor" fill-rule="evenodd"></path></svg>
          &nbsp;&nbsp;Edit on GitHub
        </a>
        <%- page.content %>
      </div>
      <div class="next-prev">
      <% if (prev) { %>
        <div class="prev-topic-wrapper">
          <a href="<%- url_for(prev.path).replace(/\.html$/, '') %>" class="button button-secondary prev-topic">
            <i class="fas fa-arrow-left prev-arrow" aria-hidden="true"></i>
            <span><%- prev.title %></span>
          </a>
        </div>
      <% } %>
      <% if (next) { %>
        <div class="next-topic-wrapper">
          <a href="<%- url_for(next.path).replace(/\.html$/, '') %>" class="button button-secondary next-topic">
            <span><%- next.title %></span>
            <i class="fas fa-arrow-right next-arrow" aria-hidden="true"></i>
          </a>
        </div>
      <% } %>
      </div>
    </div>
    <%- partial('_partial/toc') %>
  </div>
</div>
